---
import Layout from "../../layouts/Layout.astro";
import { Header50 } from "../../components/global/Header50";
import { Layout193 } from "../../components/private-events/Layout193";
import { Layout32 } from "../../components/private-events/Layout32";
import { Layout306 } from "../../components/private-events/Layout306";
import { Testimonial4 } from "../../components/private-events/Testimonial4";
import { Faq3 } from "../../components/private-events/Faq3";
import { Contact5 } from "../../components/global/Contact5";

const kicker = "Corporate Events & Private Parties";
const headline = "High Energy Live Music for Your Private Event";
const description =
  "Make your private party or corporate event unforgettable with The Benderz, the party band that keeps your guests singing and dancing all night long.";
const buttons = [
  { title: "Book Now", href: "/contact" },
  { title: "View Schedule", href: "/schedule" },
];
---

<Layout>
  <Header50
    headline={headline}
    description={description}
    kicker={kicker}
    buttons={buttons}
  />
  <Layout193 />
  <Layout32 />
  <Layout306 />
  <Testimonial4 />
  <Faq3 />
  <Contact5 />
</Layout>
