---
import Layout from "../../layouts/Layout.astro";
import Header46 from "../../components/global/Header46.astro";
import { Layout3 } from "../../components/about-us/Layout3";
import { Team14 } from "../../components/about-us/Team14";
import { Testimonial15 } from "../../components/about-us/Testimonial15";
import { Faq6 } from "../../components/about-us/Faq6";
import { ContactSection } from "../../components/global/ContactSection";

// Content
const headline = "About Us";
const description =
  "The Benderz captivate audiences with unforgettable performances, bringing together music lovers through an eclectic mix of genres.";

const contactHeading = "Contact Us";
---

<Layout>
  <Header46 headline={headline} description={description} />
  <Layout3 />
  <Team14 />
  <Testimonial15 />
  <Faq6 />
  <ContactSection client:load heading={contactHeading} />
</Layout>
